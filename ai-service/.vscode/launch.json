{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch AI Service",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "envFile": "${workspaceFolder}/.env",
      "args": ["--host", "0.0.0.0", "--port", "8000", "--reload"],
      "justMyCode": true
    }
  ]
}
