{
    "version": "0.2.0",
    "configurations": [      
        {
            "name": "Launch AI Service",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn", 
            "args": [
                "main:app", 
                "--host",
                "0.0.0.0",
                "--port",
                "8000", 
                "--reload"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/.env",
            "justMyCode": true
        }
    ]
}
